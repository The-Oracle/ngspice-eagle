NAME_TABLE:

Spice_Model_Name:      lmeter
C_Function_Name:       cm_lmeter
Description:           "ATESSE 1 compatible inductance meter"


PORT_TABLE:

Port_Name:             in               out
Description:           "input"          "output"
Direction:             in               out
Default_Type:          v                v
Allowed_Types:         [v, vd]          [v, vd, i, id]
Vector:                 no               no
Vector_Bounds:          -                -
